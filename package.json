{
	"name": "@l3dev/discord-bot-tickets-plugin",
	"version": "0.2.0",
	"description": "A plugin for discord bot tickets",
	"type": "module",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"types": "./build/index.d.ts",
			"import": "./build/index.js",
			"require": "./build/index.js"
		},
		"./commands": {
			"types": "./build/commands/index.d.ts",
			"import": "./build/commands/index.js",
			"require": "./build/commands/index.js"
		},
		"./events": {
			"types": "./build/events/index.d.ts",
			"import": "./build/events/index.js",
			"require": "./build/events/index.js"
		},
		"./db": {
			"types": "./build/db/index.d.ts",
			"import": "./build/db/index.js",
			"require": "./build/db/index.js"
		},
		"./db-schema/tickets.schema": {
			"types": "./build/db/tickets.schema.d.ts",
			"import": "./build/db/tickets.schema.js",
			"require": "./build/db/tickets.schema.js"
		}
	},
	"files": [
		"build"
	],
	"scripts": {
		"prepare": "pnpm co:login",
		"co:login": "bash auth.sh",
		"build": "tsc -p tsconfig.json",
		"package": "pnpm build",
		"prepublishOnly": "pnpm package",
		"lint": "prettier --check . && eslint .",
		"lint-fix": "prettier --check --write . && eslint . --fix",
		"format": "prettier --write ."
	},
	"author": "l3dotdev",
	"license": "MIT",
	"devDependencies": {
		"@l3dev/drizzle-helpers": "^0.2.0",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.14.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.10.0",
		"eslint-plugin-import": "^2.31.0",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1"
	},
	"dependencies": {
		"@l3dev/discord.js-helpers": "^0.5.0",
		"@l3dev/logger": "^0.2.0",
		"@l3dev/result": "^0.2.0",
		"discord.js": "^14.19.0",
		"drizzle-orm": "^0.40.1"
	}
}